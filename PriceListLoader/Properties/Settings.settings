    <Setting Name="MisDbSelectPriceByCode" Type="System.String" Scope="User">
      <Value Profile="(Default)">select first 1 S.SPRICE
    from WSCHEMA W
        join PRICESCONTRACTOR P on p.filial = @filid
                                and p.fdate &lt;= current_date
        join SCHPRICECONTRACTOR S   on w.schid = s.schid
                                    and s.prid = p.prid
                                    and s.kateg = 14 --13-individual --14-legal-entity
    where
        w.schid = (
select schid 
from wschema 
where kodoper=@kodoper
and schname=@schname
and w.structid = 3)
        and w.structid = 3
        and (W.DISDATE is null or W.DISDATE &gt; '01.01.2017')
        and (W.INVISINPRICE is null or W.INVISINPRICE = 0)
        and s.schid is not null
        and s.pricetype = 0
    order by w.schid, p.fdate desc</Value>
    </Setting>
